{
  "name": "ei-frontend",
  "version": "0.0.0",
  "private": true,
  "description": "An out-of-box UI solution for enterprise applications",
  "scripts": {
    "analyze": "cross-env ANALYZE=1 umi build",
    "build": "umi build",
    "start": "cross-env NODE_ENV=production node server.js",
    "deploy": "npm run site && npm run gh-pages",
    "fetch:blocks": "pro fetch-blocks && npm run prettier",
    "format-imports": "cross-env import-sort --write '**/*.{js,jsx,ts,tsx}'",
    "gh-pages": "cp CNAME ./dist/ && gh-pages -d dist",
    "i18n-remove": "pro i18n-remove --locale=zh-CN --write",
    "lint": "npm run lint:js && npm run lint:style && npm run lint:prettier",
    "lint-staged": "lint-staged",
    "lint-staged:js": "eslint --ext .js,.jsx,.ts,.tsx ",
    "lint:fix": "eslint --fix --cache --ext .js,.jsx,.ts,.tsx --format=pretty ./src && npm run lint:style",
    "lint:js": "eslint --cache --ext .js,.jsx,.ts,.tsx --format=pretty ./src",
    "lint:prettier": "check-prettier lint",
    "lint:style": "stylelint --fix \"src/**/*.less\" --syntax less",
    "prettier": "prettier -c --write \"**/*\"",
    "dev": "umi dev",
    "dev:no-mock": "cross-env MOCK=none umi dev",
    "dev:no-ui": "cross-env UMI_UI=none umi dev",
    "test": "umi test",
    "test:all": "node ./tests/run-tests.js",
    "test:component": "umi test ./src/components",
    "tsc": "tsc",
    "ui": "umi ui"
  },
  "lint-staged": {
    "**/*.less": "stylelint --syntax less",
    "**/*.{js,jsx,tsx,ts,less,md,json}": [
      "prettier --write",
      "git add"
    ],
    "**/*.{js,jsx}": "npm run lint-staged:js",
    "**/*.{js,ts,tsx}": "npm run lint-staged:js"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 10"
  ],
  "dependencies": {
    "@ant-design/pro-layout": "^4.11.1-0",
    "@antv/data-set": "^0.11.1",
    "antd": "^3.26.12",
    "classnames": "^2.2.6",
    "dva": "^2.4.1",
    "lodash": "^4.17.15",
    "moment": "^2.24.0",
    "omit.js": "^1.0.2",
    "path-to-regexp": "2.4.0",
    "qs": "^6.9.1",
    "react": "^16.12.0",
    "react-copy-to-clipboard": "^5.0.2",
    "react-dom": "^16.12.0",
    "react-grid-layout": "^0.18.1",
    "react-helmet": "^5.2.1",
    "redux": "^4.0.5",
    "umi": "^2.13.3",
    "umi-plugin-antd-theme": "^1.0.15",
    "umi-plugin-pro-block": "^1.3.6",
    "umi-plugin-react": "^1.15.2",
    "umi-request": "^1.2.19"
  },
  "devDependencies": {
    "@ant-design/pro-cli": "^1.0.19",
    "@types/classnames": "^2.2.9",
    "@types/express": "^4.17.2",
    "@types/history": "^4.7.5",
    "@types/jest": "^25.1.3",
    "@types/lodash": "^4.14.149",
    "@types/qs": "^6.9.1",
    "@types/react": "^16.9.23",
    "@types/react-dom": "^16.9.5",
    "@types/react-helmet": "^5.0.15",
    "@umijs/fabric": "^2.0.3",
    "chalk": "^3.0.0",
    "check-prettier": "^1.0.3",
    "cross-env": "^6.0.3",
    "cross-port-killer": "^1.2.1",
    "enzyme": "^3.11.0",
    "express": "^4.17.1",
    "gh-pages": "^2.2.0",
    "husky": "^3.1.0",
    "import-sort-cli": "^6.0.0",
    "import-sort-parser-babylon": "^6.0.0",
    "import-sort-parser-typescript": "^6.0.0",
    "import-sort-style-module": "^6.0.0",
    "jest-puppeteer": "^4.4.0",
    "jsdom-global": "^3.0.2",
    "lint-staged": "^10.0.8",
    "mockjs": "^1.1.0",
    "node-fetch": "^2.6.0",
    "prettier": "^1.19.1",
    "pro-download": "^1.0.1",
    "stylelint": "^13.2.0",
    "umi-plugin-ga": "^1.1.5",
    "umi-plugin-pro": "^1.0.2",
    "umi-types": "^0.5.12"
  },
  "optionalDependencies": {
    "puppeteer": "^2.1.1"
  },
  "engines": {
    "node": ">=10.0.0"
  },
  "checkFiles": [
    "src/**/*.js*",
    "src/**/*.ts*",
    "src/**/*.less",
    "config/**/*.js*",
    "scripts/**/*.js"
  ]
}
